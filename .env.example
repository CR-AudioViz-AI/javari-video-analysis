# ============================================
# JAVARI VIDEO ANALYSIS - ENVIRONMENT VARIABLES
# CR AudioViz AI LLC
# ============================================
# Copy this file to .env and fill in your API keys
# NEVER commit .env to version control

# Google Gemini API (from Google AI Studio)
# https://aistudio.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Twelve Labs API
# https://playground.twelvelabs.io (Settings > API Keys)
VITE_TWELVE_LABS_API_KEY=your_twelve_labs_api_key_here

# Google Cloud Video Intelligence API
# https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key_here

# Roboflow API
# https://app.roboflow.com/settings/api
VITE_ROBOFLOW_API_KEY=your_roboflow_api_key_here
VITE_ROBOFLOW_WORKSPACE=cr-audioviz-ai

# Optional: API Configuration
VITE_MAX_FILE_SIZE_MB=100
VITE_DEFAULT_API=gemini
